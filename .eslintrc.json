{
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": "standar",
  "plugins": [
    
  ],
  "rules": {
    "quotes": ["err", "single"],
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "semi": ["error", "always"],
    "comma-dangle": [
      "error",
      "always-multiline"
    ],
    "arrow-parens": [
      "error",
      "always"
    ],
    "arrow-body-style": [
      "error",
      "as-needed"
    ],
    "no-console": 1,
    "handle-callback-err": 0
  }
}